# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/tbench

# Redis
REDIS_URL=redis://localhost:6379/0

# OpenRouter (optional default)
DEFAULT_OPENROUTER_KEY=your_key_here

# File Storage
UPLOADS_DIR=uploads
JOBS_DIR=jobs

# CORS (comma-separated)
CORS_ORIGINS=["http://localhost:3000","http://localhost:3001"]

# Harbor Settings
HARBOR_TIMEOUT_MULTIPLIER=1.0
MAX_CONCURRENT_RUNS_PER_WORKER=1
